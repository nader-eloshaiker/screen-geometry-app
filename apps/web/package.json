{
  "name": "@screen-geometry-app/web",
  "version": "3.0.1",
  "private": false,
  "type": "module",
  "scripts": {
    "prepare": "pnpx msw init",
    "dev": "vite",
    "dev:e2e": "vite --mode development --port 3000",
    "preview": "vite build --watch & vite preview",
    "build": "tsc && vite build",
    "build:prod": "pnpm build",
    "build:dev": "tsc && vite build --mode development",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "lint:staged": "pnpm exec lint-staged",
    "format": "prettier --write .",
    "pre-push": "pnpm exec vitest run --changed origin/main",
    "gen:spec": "pnpm --filter=@screen-geometry-app/server-spec build && orval --config ./orval.server.config.ts",
    "gen:routes": "tsr generate",
    "gen:testbadges": "vitest --run --coverage && node ./tools/createCoverageBadges.cjs",
    "test": "vitest run --coverage.enabled=true",
    "test:watch": "vitest watch --coverage.enabled=true",
    "test:ui": "vitest watch --ui --coverage.enabled=true",
    "e2e": "pnpm exec playwright test",
    "e2e:ui": "pnpm exec playwright test --ui",
    "e2e:codegen": "pnpm exec playwright codegen localhost:3000",
    "e2e:install": "pnpm exec playwright install --with-deps",
    "e2e:report": "pnpm exec playwright show-report",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "sb": "storybook dev -p 6006",
    "sb:test": "test-storybook --coverage",
    "sb:ci": "concurrently -k -s first -n \"Storybook,Test\" -c \"magenta,blue\" \"pnpm sb\" \"wait-on tcp:6006 && pnpm sb:test\"",
    "sb:build": "storybook build",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@radix-ui/react-checkbox": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.2",
