{
  "name": "@screengeometry/web",
  "version": "3.0.1",
  "private": false,
  "type": "module",
  "scripts": {
    "pre-push": "pnpm test:ci",
    "prepare": "pnpx msw init",
    "dev": "vite",
    "dev:e2e": "vite --mode development --port 3000",
    "preview": "vite build --watch & vite preview",
    "build": "tsc && vite build",
    "build:prod": "pnpm build",
    "build:dev": "tsc && vite build --mode development",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "lint:staged": "pnpm exec lint-staged",
    "format": "prettier --write .",
    "gen:routes": "tsr generate",
    "gen:testbadges": "vitest --run --coverage && node ./tools/createCoverageBadges.cjs",
    "test": "vitest run --coverage.enabled=true",
    "test:ci": "vitest run --coverage.enabled=false",
    "test:watch": "vitest watch --coverage.enabled=true",
    "test:ui": "vitest watch --ui --coverage.enabled=true",
    "e2e": "pnpm exec playwright test",
    "e2e:ui": "pnpm exec playwright test --ui",
    "e2e:codegen": "pnpm exec playwright codegen localhost:3000",
    "e2e:install": "pnpm exec playwright install --with-deps",
    "e2e:report": "pnpm exec playwright show-report",
    "typecheck": "tsc --project tsconfig.json --noEmit"
  },
  "dependencies": {
    "@hookform/resolvers": "catalog:",
    "@react-hook/resize-observer": "^1.2.6",
    "@tanstack/eslint-plugin-query": "catalog:",
    "@tanstack/react-query": "catalog:",
    "@tanstack/react-router": "catalog:",
    "@screengeometry/lib-api": "workspace:*",
    "@screengeometry/lib-style": "workspace:*",
    "@screengeometry/lib-ui": "workspace:*",
    "@screengeometry/server": "workspace:*",
    "axios": "^1.7.9",
    "clsx": "^2.1.0",
    "framer-motion": "^11.2.10",
    "html-react-parser": "^5.2.2",
    "lucide-react": "catalog:",
    "msw": "^2.10.4",
    "react": "catalog:react18",
    "react-dom": "catalog:react18",
    "react-ga4": "^1.4.1",
    "react-hook-form": "catalog:react18",
    "simpler-color": "^1.0.2",
    "tailwindcss": "catalog:tailwindcss3",
    "tailwindcss-animate": "catalog:tailwindcss3",
    "ts-pattern": "^5.6.2",
    "ulid": "^2.3.0",
    "web-vitals": "^4.2.3",
    "yup": "catalog:"
  },
  "devDependencies": {
    "@codecov/vite-plugin": "1.9.0",
    "@eslint/compat": "^1.2.7",
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.22.0",
    "@playwright/test": "^1.54.1",
    "@tanstack/router-cli": "^1.97.14",
    "@tanstack/router-plugin": "^1.114.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.2.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint-plugin-prettier": "^3.1.3",
    "@types/jsdom": "^21.1.2",
    "@types/lint-staged": "^13.3.0",
    "@types/node": "^20.14.9",
    "@types/react": "catalog:react18",
    "@types/react-dom": "catalog:react18",
    "@types/react-helmet": "^6.1.11",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "@vitejs/plugin-react": "catalog:",
    "@vitest/coverage-istanbul": "catalog:",
    "@vitest/ui": "catalog:",
    "autoprefixer": "catalog:tailwindcss3",
    "chromatic": "^11.20.2",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-storybook": "^0.11.4",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "globals": "^16.0.0",
    "jsdom": "^23.0.1",
    "jsonfile": "^6.1.0",
    "lint-staged": "^15.2.10",
    "mkdirp": "^3.0.1",
    "postcss": "catalog:tailwindcss3",
    "postcss-import": "catalog:tailwindcss3",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "react-helmet-async": "^2.0.4",
    "styled-components": "^6.1.11",
