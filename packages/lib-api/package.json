{
  "name": "@screengeometry/lib-api",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "exports": {
    "./apiClient": "./src/axios/apiClient.ts",
    "./spec": "./src/generated/index.ts",
    "./internal": "./src/internal/index.ts"
  },
  "scripts": {
    "pre-push": "pnpm test:ci",
    "dev": "vite",
    "build": "tsc --project ./tsconfig-build.json && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:staged": "pnpm exec lint-staged",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "gen:spec": "pnpm --filter=@screengeometry/spec-server build && orval --config ./orval.server.config.ts",
    "test": "vitest run --coverage.enabled=true",
    "test:ci": "vitest run --coverage.enabled=false",
    "test:watch": "vitest watch --coverage.enabled=true",
    "test:ui": "vitest watch --ui --coverage.enabled=true"
  },
  "dependencies": {
    "@tanstack/react-query": "catalog:",
    "axios": "^1.7.9",
    "msw": "^2.10.4",
    "uuid": "^11.1.0"
  },
  "peerDependencies": {
    "react": "catalog:react18",
    "react-dom": "catalog:react18"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@tanstack/eslint-plugin-query": "catalog:",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.14.9",
    "@types/react": "catalog:react18",
    "@types/react-dom": "catalog:react18",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "@vitejs/plugin-react": "catalog:",
    "@vitest/coverage-istanbul": "catalog:",
    "@vitest/ui": "catalog:",
    "eslint": "catalog:",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.14.0",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.10",
    "orval": "^7.5.0",
    "prettier": "catalog:",
    "prettier-plugin-organize-imports": "^4.1.0",
    "react": "catalog:react18",
    "react-dom": "catalog:react18",
    "typescript": "catalog:",
    "typescript-eslint": "^8.26.1",
    "vite": "catalog:",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "catalog:",
    "vitest-mock-axios": "^0.1.0"
  }
}
