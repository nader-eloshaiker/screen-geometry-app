{
  "name": "@screengeometry/lib-api",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "exports": {
    "./apiClient": "./src/axios/apiClient.ts",
    "./spec": "./src/generated/index.ts",
    "./internal": "./src/internal/index.ts"
  },
  "scripts": {
    "pre-push": "pnpm test:ci",
    "dev": "vite",
    "build": "tsc --project ./tsconfig-build.json && vite build",
    "preview": "vite preview",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "lint:staged": "pnpm exec lint-staged",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "gen:spec:asset": "pnpm --filter=@screengeometry/spec-asset build && orval --config ./orval.asset.config.ts",
    "gen:spec:server": "pnpm --filter=@screengeometry/spec-server build && orval --config ./orval.server.config.ts",
    "gen:spec": "pnpm gen:spec:server && pnpm gen:spec:asset",
    "test": "vitest run --coverage.enabled=true",
    "test:ci": "vitest run --coverage.enabled=false",
    "test:watch": "vitest watch --coverage.enabled=true",
    "test:ui": "vitest watch --ui --coverage.enabled=true"
  },
  "dependencies": {
    "@faker-js/faker": "^9.9.0",
    "@tanstack/react-query": "catalog:",
    "axios": "^1.7.9",
    "msw": "^2.10.4",
    "uuid": "^11.1.0"
  },
  "peerDependencies": {
    "react": "catalog:react19",
    "react-dom": "catalog:react19"
  },
  "devDependencies": {
    "@eslint/js": "catalog:eslint",
    "@tanstack/eslint-plugin-query": "catalog:eslint",
    "@testing-library/jest-dom": "catalog:unittesting",
    "@testing-library/react": "catalog:unittesting",
    "@testing-library/user-event": "catalog:unittesting",
    "@types/node": "^24.2.0",
    "@types/react": "catalog:react19",
    "@types/react-dom": "catalog:react19",
    "@vitejs/plugin-react": "catalog:vite",
    "@vitest/coverage-istanbul": "catalog:vite",
    "@vitest/ui": "catalog:vite",
    "eslint": "catalog:eslint",
    "eslint-config-prettier": "catalog:eslint",
    "eslint-plugin-import": "catalog:eslint",
    "eslint-plugin-prettier": "catalog:eslint",
    "eslint-plugin-react": "catalog:eslint",
    "eslint-plugin-react-hooks": "catalog:eslint",
    "eslint-plugin-react-refresh": "catalog:eslint",
    "eslint-plugin-storybook": "catalog:eslint",
    "globals": "catalog:eslint",
    "jsdom": "^26.1.0",
    "lint-staged": "catalog:",
    "orval": "catalog:",
    "prettier": "catalog:",
    "prettier-plugin-organize-imports": "^4.1.0",
    "react": "catalog:react19",
    "react-dom": "catalog:react19",
    "typescript": "catalog:",
    "typescript-eslint": "catalog:eslint",
    "vite": "catalog:vite",
    "vite-tsconfig-paths": "catalog:vite",
    "vitest": "catalog:vite",
    "vitest-mock-axios": "catalog:unittesting"
  }
}
