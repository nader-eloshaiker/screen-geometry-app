{
  "name": "screen-geometry-app",
  "version": "3.0.1",
  "homepage": "https://github.com/nader-eloshaiker/screen-geometry-app",
  "private": false,
  "license": "MIT",
  "type": "module",
  "author": {
    "name": "Nader Eloshaiker",
    "url": "https://github.com/nader-eloshaiker"
  },
  "packageManager": "pnpm@9.1.3",
  "engines": {
    "node": ">=20",
    "pnpm": ">=9"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "updatemsw": "pnpx msw init",
    "dev": "vite",
    "dev:e2e": "vite --mode development --port 3000",
    "preview": "vite build --watch & vite preview",
    "build": "tsc && vite build",
    "build:prod": "pnpm build",
    "build:dev": "tsc && vite build --mode development",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --fix --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write .",
    "gen:spec": "orval --config ./orval.config.ts",
    "gen:routes": "tsr generate",
    "gen:testbadges": "vitest --run --coverage && node ./tools/createCoverageBadges.cjs",
    "test": "vitest --coverage",
    "test:ui": "vitest --ui --coverage.enabled=true",
    "test:ci": "vitest --run --coverage",
    "e2e": "pnpm exec playwright test",
    "e2e:ui": "pnpm exec playwright test --ui",
    "e2e:codegen": "pnpm exec playwright codegen localhost:3000",
    "e2e:install": "pnpm exec playwright install --with-deps",
    "e2e:report": "pnpm exec playwright show-report",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "sb": "storybook dev -p 6006",
    "sb:test": "test-storybook --coverage",
    "sb:ci": "concurrently -k -s first -n \"Storybook,Test\" -c \"magenta,blue\" \"pnpm sb\" \"wait-on tcp:6006 && pnpm sb:test\"",
    "sb:build": "storybook build",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@radix-ui/react-checkbox": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.2",
    "@radix-ui/react-scroll-area": "^1.2.2",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.2",
    "@radix-ui/react-toggle": "^1.1.0",
    "@radix-ui/react-toggle-group": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.4",
    "@react-hook/resize-observer": "^1.2.6",
    "@tanstack/react-query": "^5.67.1",
    "@tanstack/react-query-devtools": "^5.67.1",
    "@tanstack/react-router": "^1.97.14",
    "@tanstack/router-devtools": "^1.97.14",
    "axios": "^1.7.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cmdk": "^1.0.4",
    "framer-motion": "^11.2.10",
    "fuzzysort": "^3.0.2",
    "html-react-parser": "^5.2.2",
    "lucide-react": "^0.453.0",
    "msw": "^2.7.0",
    "react": "^18.3.1",
    "react-dom": "^18.2.0",
    "react-ga4": "^1.4.1",
    "react-hook-form": "^7.54.2",
    "simpler-color": "^1.0.1",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "ts-pattern": "^5.4.0",
    "ulid": "^2.3.0",
    "vaul": "^1.1.2",
    "web-vitals": "^4.2.3",
    "yup": "^1.4.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@bgotink/playwright-coverage": "^0.2.2",
    "@codecov/vite-plugin": "1.9.0",
    "@playwright/test": "^1.47.1",
    "@storybook/addon-coverage": "^1.0.4",
    "@storybook/addon-essentials": "^8.4.6",
    "@storybook/addon-interactions": "^8.4.6",
    "@storybook/addon-themes": "^8.4.6",
    "@storybook/addon-viewport": "^8.4.6",
    "@storybook/react": "^8.4.6",
    "@storybook/react-vite": "^8.4.6",
    "@storybook/test": "^8.4.6",
    "@storybook/test-runner": "^0.19.1",
    "@tanstack/router-cli": "^1.97.14",
    "@tanstack/router-plugin": "^1.97.14",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^14.2.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint-plugin-prettier": "^3.1.3",
    "@types/jsdom": "^21.1.2",
    "@types/lint-staged": "^13.3.0",
    "@types/node": "^20.14.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.2.17",
    "@types/react-helmet": "^6.1.11",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "7.18.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-istanbul": "^3.0.7",
    "@vitest/ui": "^3.0.7",
    "autoprefixer": "^10.4.17",
    "chromatic": "^11.20.2",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "fake-indexeddb": "^6.0.0",
    "husky": "^9.1.1",
    "jsdom": "^23.0.1",
    "jsonfile": "^6.1.0",
    "lint-staged": "^15.2.10",
    "mkdirp": "^3.0.1",
    "orval": "^7.1.1",
    "postcss": "^8.5.3",
    "postcss-import": "^16.1.0",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^3.2.4",
    "react-helmet-async": "^2.0.4",
    "storybook": "^8.4.6",
    "storybook-addon-pseudo-states": "^4.0.2",
    "styled-components": "^6.1.11",
    "tailwind-merge": "^2.4.0",
    "tailwind-styled-components": "^2.2.0",
    "typescript": "^5.3.3",
    "vite": "^6.2.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.7",
    "vitest-localstorage-mock": "^0.1.2",
    "wait-on": "^7.2.0"
  },
  "browserslist": [
    "defaults"
  ],
  "lint-staged": {
    "**/*.{ts,tsx,js,json,yaml,yml} !(src/openapi/generated/**/*)": "npm run lint:fix",
    "*.{ts,tsx,js,csss,json,yaml,yml,md*}": "npm run format"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
